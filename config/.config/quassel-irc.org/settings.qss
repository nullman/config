// Style settings made in Quassel's configuration dialog
// This file is automatically generated, do not edit

// ChatView Font
ChatLine { font: normal 400 11pt "Monospace"; }

// Custom ChatView Colors
Palette { marker-line: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #a51d2d, stop: 0.1 transparent); }
ChatView { background: #241f31; }

ChatLine[label="highlight"] {
  foreground: #3d3846;
  background: #99c1f1;
}

ChatLine::timestamp { foreground: #5e5c64; }

ChatLine#plain { foreground: #f6f5f4; }
ChatLine#notice { foreground: #b5835a; }
ChatLine#action { foreground: #62a0ea; }
ChatLine#nick { foreground: #9a9996; }
ChatLine#mode { foreground: #9a9996; }
ChatLine#join { foreground: #9a9996; }
ChatLine#part { foreground: #9a9996; }
ChatLine#quit { foreground: #9a9996; }
ChatLine#kick { foreground: #9a9996; }
ChatLine#kill { foreground: #9a9996; }
ChatLine#server { foreground: #b5835a; }
ChatLine#info { foreground: #b5835a; }
ChatLine#error { foreground: #ed333b; }
ChatLine#daychange { foreground: #b5835a; }
ChatLine#topic { foreground: #9a9996; }
ChatLine#netsplit-join { foreground: #9a9996; }
ChatLine#netsplit-quit { foreground: #9a9996; }
ChatLine#invite { foreground: #9a9996; }


// Sender Colors
Palette {
    sender-color-self: #000000;
    sender-color-00: #cc0000;
    sender-color-01: #006cad;
    sender-color-02: #4d9900;
    sender-color-03: #6600cc;
    sender-color-04: #a67d00;
    sender-color-05: #009927;
    sender-color-06: #0030c0;
    sender-color-07: #cc009a;
    sender-color-08: #b94600;
    sender-color-09: #869900;
    sender-color-0a: #149900;
    sender-color-0b: #009960;
    sender-color-0c: #006cad;
    sender-color-0d: #0099cc;
    sender-color-0e: #b300cc;
    sender-color-0f: #cc004d;
}

ChatLine::sender#plain[sender="self"] { foreground: palette(sender-color-self); }

ChatLine::sender#plain[sender="00"] { foreground: palette(sender-color-00); }
