#!/usr/bin/env bash
#===============================================================================
# audacious-enqueue
#
# Launch AUDACIOUS and enqueue any passed files.
#
# Author: Kyle W T Sherman <kylewsherman@gmail.com>
#===============================================================================

if command -v audacious ; then
    audacious --enqueue "$@"
elif command -v audacious2 ] ; then
    audacious2 --enqueue "$@"
fi

#===============================================================================
# End of File
#===============================================================================
