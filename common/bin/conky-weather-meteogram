#!/usr/bin/env bash
finger "^san_diego@graph.no" | head -n -1 | tail -n +2 | \
    sed "s/^   -=/   %%/ ; s/=-   $/%%   / ; s/=V=//g ; \
    s/-//g ; s/\^//g ; s/=//g ; s/#//g ; \
    s/|//g ; s/!//g ; s/\*//g ; \
    s/^   %%/   -=/ ; s/%%   $/=-   / ; \
    s/^\(    .*\)$/\${color2}\1\${color}/ ; \
    s/^ 'F\(.*\)$/\${color2} °F\1\${color}/ ; \
    s/^\( [0-9]\{2\}\)/\${color2}\1\${color}/ ; \
    s/^\(.*:\)\(.*\)$/\${color2}\1\${color}\2/ ; \
    s/^\(   -=.*\)$/\${font MesloLGS Nerd Font:bold:size=12}\1\${font}/"
