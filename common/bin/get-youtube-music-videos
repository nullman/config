fetch() {
    local dir="$1"
    shift
    local list=("${@}")
    mkdir -p "${dir}"
    for ((x = 0; x < ${#list[@]}; x += 1)) ; do
        video=$(echo "${list[$x]}" | sed 's/ *|/|/g ; s/| */|/g')
        name=$(echo "${video}" | cut -d '|' -f 1)
        url=$(echo "${video}" | cut -d '|' -f 2)
        escname=$(echo ${name} | sed 's/\[/\\\[/g ; s/\]/\\\]/g')
        echo "| ${name} | ${url} |"
        if [[ -z $(find "${dir}" -name "${escname}\.*") ]] ; then
            cmd="${ytdlp} \
            --cookies-from-browser brave \
            --format-sort 'res:1080,res:720,res:480,vcodec:h264,acodec:m4a' \
            --paths \"temp:/tmp\" \
            --paths \"home:${dir}\" \
            --output \"${name} - %(release_date>%Y-%m-%d,upload_date>%Y-%m-%d|Unknown)s - %(title)s.%(ext)s\" \
            --output \"${name}.%(ext)s\" \
            '${url}'"
            echo
            echo "${url}"
            echo
            eval "${cmd}"
        fi
    done
}

fetch "${basedir}/Video" "${videolist[@]}"
fetch "${basedir}/Ambient/General" "${ambientlist[@]}"

exit 0

