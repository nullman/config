[Desktop Entry]
Version=1.0
Type=Application
Name=Rotate HDMI-1 Right
Comment=Send xrandr command to rotate HDMI-1 screen to the right position
Exec=sh -c 'xrandr --output HDMI-1 --rotate right'
Icon=go-next
Terminal=false
StartupNotify=false
Categories=Accessibility;Screen;

